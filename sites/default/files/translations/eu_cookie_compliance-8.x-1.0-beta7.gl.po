msgid "Remove"
msgstr "Quitar"
msgid "Settings"
msgstr "Axustes"
msgid "Export"
msgstr "Exportar"
msgid "Text Color"
msgstr "Cor do texto"
msgid "Background Color"
msgstr "Cor de fondo"
msgid "Add"
msgstr "Engadir"
msgid "Domain"
msgstr "Dominio"
